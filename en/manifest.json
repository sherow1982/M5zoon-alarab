    },
    {
      "src": "../images/screenshot-mobile.png",
      "sizes": "390x844",
      "type": "image/png",
      "platform": "narrow",
      "label": "Emirates Gifts Mobile Experience"
    }
  ],
  "shortcuts": [
    {
      "name": "Shop Perfumes",
