      "icons": [
        {
          "src": "../images/icon-perfumes.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Shop Watches",
      "short_name": "Watches",
      "description": "Discover luxury timepieces and accessories",
      "url": "/en/#watches-section",
      "icons": [
        {
          "src": "../images/icon-watches.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "WhatsApp Order",
      "short_name": "WhatsApp",
      "description": "Order directly via WhatsApp for instant service",
      "url": "https://wa.me/201110760081",
      "icons": [
        {
          "src": "../images/icon-whatsapp.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Cart",
      "short_name": "Cart",
      "description": "Review your selected items and checkout",
      "url": "/en/cart.html",
      "icons": [
        {
          "src": "../images/icon-cart.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://emirates-gifts.arabsad.com/en/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+emiratesgifts",
      "url": "/en/?action=%s"
    }
  ],
  "share_target": {
    "action": "/en/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [],
  "iarc_rating_id": "",
  "offline_enabled": true,
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "css/*",
        "js/*",
        "images/*",
        "../data/*"
      ],
      "matches": [
        "https://emirates-gifts.arabsad.com/*"
      ]
    }
  ]
}